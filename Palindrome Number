class Solution {
    public boolean isPalindrome(int x) {
        int value = x;
        if(x < 0){
            return false;
        }
        int res = 0;
        while(x != 0){
            res = res * 10 + x%10;
            x = x/10;
        }
        if(res == value){
            return true;
        }
        return false;
    }
}
